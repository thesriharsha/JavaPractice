package static_and_final_exercises;

record Circle(int x, int y, int radius) {
	
	public double area() {
		return 3.14*radius*radius;
	}

public class TestCircle {

	public static void main(String[] args) {
		
		var c1 = new Circle(5,4,3);
		// Getter Methods
		System.out.println(c1.x());
		System.out.println(c1.x());
		System.out.println(c1.x());
		

	}

}
